<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffffff">

        <TextView android:id="@+id/frc_tv_city_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/city"
            android:textAllCaps="true"
            android:textColor="@color/colorAsh"
            android:textSize="10sp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginTop="24dp" />

        <LinearLayout android:id="@+id/frc_ll1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginLeft="24dp"
            android:layout_marginRight="24dp"
            android:layout_below="@+id/frc_tv_city_title">

            <TextView android:id="@+id/frc_tv_city"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:text="@string/visakhapatnam"
                android:textColor="@color/colorTextBlue"
                android:textStyle="bold"
                android:textSize="14sp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="4dp"/>
            <ImageButton android:id="@+id/frc_ib_city"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/m_edit"
                android:background="@color/colorWhite"
                android:visibility="gone"/>

        </LinearLayout>

        <TextView android:id="@+id/frc_tv_category_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/category"
            android:textAllCaps="true"
            android:textColor="@color/colorAsh"
            android:textSize="10sp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginTop="12dp"
            android:layout_below="@+id/frc_ll1"/>

        <LinearLayout android:id="@+id/frc_ll2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginLeft="24dp"
            android:layout_marginRight="24dp"
            android:layout_below="@+id/frc_tv_category_title">

            <TextView android:id="@+id/frc_tv_category"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:text="@string/mini"
                android:textColor="@color/colorTextBlue"
                android:textStyle="bold"
                android:textSize="14sp"
                android:layout_gravity="center"
                android:layout_weight="1"/>
            <ImageButton android:id="@+id/frc_ib_category"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/m_edit"
                android:background="@color/colorWhite"/>

        </LinearLayout>



        <TextView android:id="@+id/frc_tv_travel_type_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/travel_type"
            android:textAllCaps="true"
            android:textColor="@color/colorAsh"
            android:textSize="10sp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginTop="12dp"
            android:layout_below="@+id/frc_ll2"/>
        <!--

       <LinearLayout android:id="@+id/frc_ll3"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:orientation="horizontal"
           android:layout_marginLeft="24dp"
           android:layout_marginRight="24dp"
           android:layout_below="@+id/frc_tv_travel_type_title">

           <TextView android:id="@+id/frc_tv_travel_type"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:gravity="left"
               android:text="@string/local"
               android:textColor="@color/colorTextBlue"
               android:textStyle="bold"
               android:textSize="14sp"
               android:layout_gravity="center"
               android:layout_weight="1"/>
           <ImageButton android:id="@+id/frc_ib_travel_type"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:src="@drawable/m_edit"
               android:background="@color/colorWhite"/>

       </LinearLayout>
       -->

        <LinearLayout android:id="@+id/frc_ll3"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="32dp"
            android:layout_marginRight="32dp"
            android:layout_marginTop="12dp"
            android:layout_below="@+id/frc_tv_travel_type_title">

            <TextView android:id="@+id/frc_tv_local"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/local"
                android:textAllCaps="true"
                android:textSize="12sp"
                android:textColor="@color/colorTextBlue"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:gravity="center"
                android:background="@drawable/rect_blue_stroke_bg_nc"
                android:padding="6dp"/>

            <TextView android:id="@+id/frc_tv_outstation"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/out_station"
                android:textAllCaps="true"
                android:textSize="12sp"
                android:textColor="@color/colorAsh"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:gravity="center"
                android:background="@drawable/rect_ash_stroke_bg_single_line"
                android:padding="6dp"/>



            </LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/frc_ll3"
            android:layout_marginTop="24dp"
            android:layout_marginBottom="24dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:background="@drawable/rect_ash_stroke_bg"
            android:id="@+id/relativeLayout">

            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:orientation="vertical" android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="16dp"
                android:paddingBottom="16dp">

                <TextView android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/standard_fare"
                    android:textAllCaps="true"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    android:gravity="left"
                    android:layout_marginTop="14dp"
                    android:layout_marginBottom="16dp"
                    android:layout_marginLeft="24dp"
                    android:textColor="@color/colorBlack"/>


                <LinearLayout android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingTop="12dp"
                    android:paddingBottom="12dp"
                    android:paddingLeft="24dp"
                    android:paddingRight="24dp">
                    <TextView android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/base_fare"
                        android:textColor="@color/colorAsh"
                        android:textSize="14sp"
                        android:gravity="left"/>
                    <TextView android:id="@+id/frc_tv_base_fare"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Rs. - "
                        android:textColor="@color/colorBlack"
                        android:textSize="14sp"
                        android:gravity="right"/>
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorAshLight"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>

                <LinearLayout android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingTop="12dp"
                    android:paddingBottom="12dp"
                    android:paddingLeft="24dp"
                    android:paddingRight="24dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/bslab"
                        android:textColor="@color/colorAsh"
                        android:textSize="14sp"
                        android:gravity="left"/>
                    <TextView android:id="@+id/frc_tv_below"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Rs. - per km"
                        android:textColor="@color/colorBlack"
                        android:textSize="14sp"
                        android:gravity="right"/>



                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorAshLight"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>
                <LinearLayout android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingTop="12dp"
                    android:paddingBottom="12dp"
                    android:paddingLeft="24dp"
                    android:paddingRight="24dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/aslab"
                        android:textColor="@color/colorAsh"
                        android:textSize="14sp"
                        android:gravity="left"/>
                    <TextView  android:id="@+id/frc_tv_above"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Rs. - per km"
                        android:textColor="@color/colorBlack"
                        android:textSize="14sp"
                        android:gravity="right"/>



                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorAshLight"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>
                <LinearLayout android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingTop="12dp"
                    android:paddingBottom="12dp"
                    android:paddingLeft="24dp"
                    android:paddingRight="24dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/ride_time_charges"
                        android:textColor="@color/colorAsh"
                        android:textSize="14sp"
                        android:gravity="left"/>
                    <TextView android:id="@+id/frc_tv_ride_time_charges"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Rs. - per min"
                        android:textColor="@color/colorBlack"
                        android:textSize="14sp"
                        android:gravity="right"/>
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorAshLight"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>

                <LinearLayout android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingTop="12dp"
                    android:paddingBottom="12dp"
                    android:paddingLeft="24dp"
                    android:paddingRight="24dp">
                    <TextView android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/per_km_fare"
                        android:textColor="@color/colorAsh"
                        android:textSize="14sp"
                        android:gravity="left"/>
                    <TextView android:id="@+id/frc_tv_per_km_fare"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Rs. - "
                        android:textColor="@color/colorBlack"
                        android:textSize="14sp"
                        android:gravity="right"/>
                </LinearLayout>

                <View android:id="@+id/frc_vw_per_km_fare"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorAshLight"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>
                <LinearLayout android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingTop="12dp"
                    android:paddingBottom="12dp"
                    android:paddingLeft="24dp"
                    android:paddingRight="24dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/min_fare"
                        android:textColor="@color/colorAsh"
                        android:textSize="14sp"
                        android:gravity="left"/>
                    <TextView android:id="@+id/frc_tv_min_fare"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Rs. - "
                        android:textColor="@color/colorBlack"
                        android:textSize="14sp"
                        android:gravity="right"/>
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorAshLight"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>
                <LinearLayout android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingTop="12dp"
                    android:paddingBottom="12dp"
                    android:paddingLeft="24dp"
                    android:paddingRight="24dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/peak_time_charges"
                        android:textColor="@color/colorAsh"
                        android:textSize="14sp"
                        android:gravity="left"/>
                    <TextView  android:id="@+id/frc_tv_peak_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text=" - "
                        android:textColor="@color/colorBlack"
                        android:textSize="14sp"
                        android:gravity="right"
                        android:layout_marginTop="8dp"/>
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorAshLight"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>
                <LinearLayout android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingTop="12dp"
                    android:paddingBottom="12dp"
                    android:paddingLeft="24dp"
                    android:paddingRight="24dp">
                    <TextView android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/service_tax"
                        android:textColor="@color/colorAsh"
                        android:textSize="14sp"
                        android:gravity="left"/>
                    <TextView android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text=" - "
                        android:textColor="@color/colorBlack"
                        android:textSize="14sp"
                        android:gravity="right"
                        android:layout_marginTop="8dp"/>
                </LinearLayout>


            </LinearLayout>
        </RelativeLayout>

    </RelativeLayout>
</ScrollView>